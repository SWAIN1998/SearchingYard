{
    "version": 2,
    "builds":[
        {
            "src":"./index.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/",
            "dest":"/api/index.js"
        },
        {
            "src":"/api/(.*)",
            "dest":"/api/$1"
        }
    ]
}